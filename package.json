{
  "name": "usc-backend",
  "version": "1.0.0",
  "description": "USC backend ptototype",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "npx tsc --outDir ./dist",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "lint": "eslint src/**/*.{js,ts,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,ts,json}'",
    "format": "prettier --write 'src/**/*.{js,ts,md,json}' --config ./.prettierrc.json",
    "migrate": "prisma migrate dev",
    "deploy": "prisma migrate deploy && prisma generate",
    "seed": "prisma db seed"
  },
  "prisma": {
    "seed": "node src/seed.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^7.6.0",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.13",
    "babel-jest": "^29.3.1",
    "concurrently": "^7.6.0",
    "jest-mock-extended": "^2.0.4",
    "nodemon": "^2.0.20",
    "prettier": "2.8.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.4",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^4.7.1"
  }
}
